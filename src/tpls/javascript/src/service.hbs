// Required collections
require('{{filePath models.get}}');
// ---------------------------- /
const AmjsAjaxService = require('@amjs/ajax-service');

/**
* @NOTICE: DO NOT MODIFY THIS FILE, generated with '@amjs/api-parser'
* @created {{date}}
*
* {{service.summary}}
* @namespace   {{dotSep namespace}}
* @class       {{dotSep namespace}}.{{key}}
* @extends     amjs.ajax.Service
*/
class {{className}} extends AmjsAjaxService
{
    /**
     * @override
     */
    constructor(values = {})
    {
        super();

        this.$allowedMethods = [
            {{#each allowedMethods as |method|}}
            '{{method}}',
            {{/each}}
        ];

        values = Object.assign(values, {
            path   : '{{url}}'
        });

        this._setProperties(values);
    }

    /**
     * @override
     */
    _getModel(method = 'GET')
    {
        let model;
        switch (method)
        {
            {{#if models.post}}
            case 'POST':
                model = '{{models.post}}';
                break;
            {{/if}}
            {{#if models.put}}
            case 'PUT':
                model = '{{models.put}}';
                break;
            {{/if}}
            {{#if models.patch}}
            case 'PATCH':
                model = '{{models.patch}}';
                break;
            {{/if}}
            {{#if models.delete}}
            case 'DELETE':
                model = '{{models.delete}}';
                break;
            {{/if}}
            case 'GET':
            default:
                model = '{{models.get}}';
                break;
        }

        return model;
    }
}

// Register & export service class
AmjsAjaxService.register('{{className}}', {{className}});
module.exports = {{className}};